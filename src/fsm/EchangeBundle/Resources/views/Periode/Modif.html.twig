{% block body %}
{#On n'affiche le formulaire que s'il existe#}
{#Lors du 1er apel à la liste catégorie, on ne visionne qu'une liste. Pas de formulaire#}
{% if form != null%}
<div class ="well">
      <form method ="post" {{ form_enctype(form) }}
            action = "{{ path('fsm_periode_update',{'id':id }) }}" > 

     {{ form_row(form.libelle) }}
       {{ form_rest(form) }}
<input type ="submit" class ="btn btn-primary" />
</form>
</div>
{% endif%}
{% endblock %}

